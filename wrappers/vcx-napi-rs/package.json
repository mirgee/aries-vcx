{
  "package": "vcx-napi-rs",
  "devDependencies": {
    "@napi-rs/cli": "^2.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build:ts": "tsc -p ./tsconfig.json",
    "build:napi": "napi build --cargo-cwd ./api-node/ --release .",
    "build:napi:debug": "napi build --cargo-cwd ./api-node/ ."
  },
  "napi": {
    "name": "index",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  }
}
