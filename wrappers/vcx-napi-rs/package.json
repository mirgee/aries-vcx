{
  "name": "@hyperledger/vcx-napi-rs",
  "package": "@hyperledger/vcx-napi-rs",
  "devDependencies": {
    "@napi-rs/cli": "^2.9.1",
    "@types/node": "^18.11.18",
    "typescript": "^4.8.4"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build:ts": "tsc -p ./tsconfig.json",
    "build:napi": "napi build --platform --release",
    "build:napi:debug": "napi build --platform --features test_utils",
    "create-npm-dir": "napi create-npm-dir -t ."
  },
  "napi": {
    "name": "vcx-napi-rs",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  }
}
